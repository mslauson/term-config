# Created by Zap installer
[ -f "${XDG_DATA_HOME:-$HOME/.local/share}/zap/zap.zsh" ] && source "${XDG_DATA_HOME:-$HOME/.local/share}/zap/zap.zsh"

plug "zsh-users/zsh-autosuggestions"
plug "zap-zsh/supercharge"
plug "zap-zsh/zap-prompt"
plug "zsh-users/zsh-syntax-highlighting"
plug "Aloxaf/fzf-tab"
# plug "lukechilds/zsh-nvm"
plug "akash329d/zsh-alias-finder"
plug "KulkarniKaustubh/fzf-dir-navigator"
plug "joshskidmore/zsh-fzf-history-search"
plug "reegnz/jq-zsh-plugin"
# plug "wintermi/zsh-bob"

# source ~/.zsh/plugins/nvim.zsh

source ~/.zsh/themes/catppuccin_mocha-zsh-syntax-highlighting.zsh

# Bindings for Ctrl + Left and Ctrl + Right
bindkey '\e[1;5D' backward-word  # Ctrl + Left
bindkey '\e[1;5C' forward-word  # Ctrl + Right

# Bindings for Home and End
bindkey '\e[1;9D' beginning-of-line  # Home
bindkey '\e[1;9C' end-of-line  # End

#Delete Key
bindkey '\e[3~' delete-char

zmodload zsh/zpty
# completions
if [ -d $HOME/.zsh/comp ]; then
    export FPATH="$HOME/.zsh/comp:$FPATH"
fi
